#: main.cpp:46
msgid "KIO Exec - Opens remote files, watches modifications, asks for upload"
msgstr ""
"برنامج KIO Exec - يقوم بفتح الملفات البعيدة ومراقبة أية تعديلات عليها ، ثم "
"يسألك عن إرسال التعديلات."

#: main.cpp:54
msgid "'command' expected.\n"
msgstr "متوقّع 'الأمر'.\n"

#: main.cpp:83
#, kde-format
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"العنوان %1\n"
"مشوّه"

#: main.cpp:85
#, kde-format
msgid ""
"Remote URL %1\n"
"not allowed with --tempfiles switch"
msgstr ""
"العنوان البعيد %1\n"
"غير مسموح به مع مبدّل --tempfiles"

#: main.cpp:218
#, kde-format
msgid ""
"The supposedly temporary file\n"
"%1\n"
"has been modified.\n"
"Do you still want to delete it?"
msgstr ""
"الملف المؤقت المفترض\n"
"%1\n"
"تم تعديله\n"
"هل تريد حذفه؟"

#: main.cpp:219 main.cpp:226
msgid "File Changed"
msgstr "لقد تغيَر الملف"

#: main.cpp:219
msgid "Do Not Delete"
msgstr "لا تحذف"

#: main.cpp:225
#, kde-format
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"الملف\n"
"%1\n"
"تم تعديله\n"
"هل تريد إرسال التغييرات؟"

#: main.cpp:226
msgid "Upload"
msgstr "ارسل"

#: main.cpp:226
msgid "Do Not Upload"
msgstr "لا ترسل"

