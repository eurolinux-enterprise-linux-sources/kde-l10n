#: kfmclient.cpp:55
msgid "kfmclient"
msgstr "kfmclient"

#: kfmclient.cpp:56
msgid "KDE tool for opening URLs from the command line"
msgstr "أداة كيدي لفتح العناوين من سطر الأوامر"

#: kfmclient.cpp:74
msgid "Non interactive use: no message boxes"
msgstr "لا استخدام تبادلي: لا مربعات مسائلة"

#: kfmclient.cpp:76
msgid "Show available commands"
msgstr "أظهر الأوامر المتوفرة"

#: kfmclient.cpp:78
msgid "Command (see --commands)"
msgstr "الأمر (انظر --الأوامر)"

#: kfmclient.cpp:80
msgid "Arguments for command"
msgstr "معطيات الأمر"

#: kfmclient.cpp:90
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"الجملة:\n"

#: kfmclient.cpp:91
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # تفتح نافذة تظهر 'url'.\n"
"            #  'url' ربما يكون مسار نسبي \n"
"            #   أو اسم ملف, مثلاً . أو دليل فرعي/\n"
"            #   إذا اسقط 'url' , استخدم$HOME بدلاً من ذلك.\n"
"\n"

#: kfmclient.cpp:96
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that Konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # إذا تم تحديد 'mimetype', فإنه سيستخدم لمعرفة المكوّن الذي يجب\n"
"            #   أن يستخدمه كونكيورر. مثلاً استخدم text/html لصفحة شبكية, "
"لجعلها\n"
"             #   تظهر بشكل أسرع\n"
"\n"

#: kfmclient.cpp:100
msgid ""
"  kfmclient newTab 'url' ['mimetype']\n"
"            # Same as above but opens a new tab with 'url' in an existing "
"Konqueror\n"
"            #   window on the current active desktop if possible.\n"
"\n"
msgstr ""
"  kfmclient newTab 'url' ['mimetype']\n"
"            # بالطريقة نفسها ولكن يفتح لسان جديد بواسطة 'url' في كونكيورر "
"مسبقاً\n"
"            #   نافذة على سطح المكتب النشط إذا كان ممكناً.\n"
"\n"

#: kfmclient.cpp:104
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under ~/.kde/share/apps/konqueror/"
"profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # يفتح نافذة باستخدام المواصفات المعطاة.\n"
"            #   'profile' هو ملف موجود في ~/.kde/share/apps/konqueror/"
"profiles.\n"
"            #   'url' هو عنوان اختياري للفتح.\n"
"\n"

#: kfmclient.cpp:449
#, kde-format
msgid "Profile %1 not found\n"
msgstr "المواصفات %1 غير موجودة\n"

#: kfmclient.cpp:480
msgid "Syntax Error: Not enough arguments\n"
msgstr "خطأ في بناء الجملة: المعطيات ليست كافية\n"

#: kfmclient.cpp:485
msgid "Syntax Error: Too many arguments\n"
msgstr "خطأ في بناء الجملة: المعطيات كثيرة\n"

#: kfmclient.cpp:552
#, kde-format
msgid "Syntax Error: Unknown command '%1'\n"
msgstr "خطأ في بناء الجملة: الأمر مجهول '%1'\n"
