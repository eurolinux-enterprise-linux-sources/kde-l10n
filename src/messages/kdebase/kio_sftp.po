#~ msgstr "من فضلك أدخل اسم المستخدم و كلمة السر."

#~ msgid "File does not exist."
#~ msgstr "الملف غير موجود"

#~ msgid "Access is denied."
#~ msgstr "النفاذ ممنوع"
