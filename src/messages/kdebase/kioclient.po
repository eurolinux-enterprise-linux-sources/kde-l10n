msgid "URL or URLs"
msgstr "عنوان أو عناوين"

#: kioclient.cpp:107 kioclient.cpp:110
msgid "Source URL or URLs"
msgstr "مصدر العنوان أو العناوين"

#: kioclient.cpp:108 kioclient.cpp:111
msgid "Destination URL"
msgstr "وجهة العنوان"

#: kioclient.cpp:113
